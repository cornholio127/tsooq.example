{
  "name": "tsooq.example",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "start": "npm run build:live",
    "build:live": "nodemon --exec ./node_modules/.bin/ts-node -- ./src/server.ts",
    "build": "npm run build-ts && npm run tslint",
    "serve": "node dist/server.js",
    "build-ts": "npx tsc",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "gen": "tsooq-gen",
    "test": "mocha --require ts-node/register test/*.ts"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "express": "4.17.3",
    "tsooq": "0.1.11"
  },
  "devDependencies": {
    "@types/node": "12.7.4",
    "@types/express": "4.17.1",
    "@types/compression": "0.0.36",
    "@types/pg": "7.11.1",
    "typescript": "3.6.3",
    "tsooq-gen": "0.1.4",
    "tslint": "5.20.0",
    "ts-node": "7.0.1",
    "nodemon": "1.19.1",
    "cross-env": "6.0.0",
    "mocha": "6.2.0",
    "chai": "4.2.0",
    "sinon": "7.4.1",
    "@types/mocha": "5.2.7",
    "@types/chai": "4.2.2",
    "@types/sinon": "4.3.1"
  }
}
